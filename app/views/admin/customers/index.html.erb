<h2>会員一覧</h2>

  <table>
   <tr>
    <td>会員ID</td>
    <td>氏名</td>
    <td>メールアドレス</td>
    <td>ステータス</td>
   </tr>
  </table>

<% @customers.each do |customer| %>
  <table>
   <tr>
    <td>
     <%= customer.id %>
    </td>
    <td>
      <%= link_to admin_customer_path(customer.id) do %>
       <%= customer.last_name %><%= customer.first_name %>
      <% end %>
    </td>
    <td><%= customer.email %></td>
    <td><%= customer.is_active_i18n %></td>
   </tr>
  </table>
 <% end %>